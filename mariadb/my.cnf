[client]
port		= 3306
socket		= /var/run/mysqld/mysqld.sock

[mysqld]
port		= 3306
socket		= /var/run/mysqld/mysqld.sock
key_buffer_size=16K
max_allowed_packet=8M
thread_stack=128K
table_cache=4
sort_buffer_size=64K
net_buffer_length=2K
performance_schema=off

innodb_buffer_pool_size=16M
innodb_log_file_size=5M
innodb_log_buffer_size=8M